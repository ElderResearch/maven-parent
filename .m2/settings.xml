<?xml version="1.0" encoding="UTF-8"?>
<settings
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
	xmlns="http://maven.apache.org/SETTINGS/1.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<servers>
		<server>
			<id>eri-releases</id>
			<username>erminion</username>
			<password>${env.ARTIFACTORY_API_KEY}</password>
		</server>
	</servers>
	<profiles>
		<profile>
			<id>eri-releases</id>
			<activation><activeByDefault/></activation>
			<repositories>
				<repository>
					<id>eri-releases</id>
					<url>https://elderresearch.jfrog.io/elderresearch/eri-releases</url>
				</repository>
			</repositories>
		</profile>
	</profiles>
</settings>
