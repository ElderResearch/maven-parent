<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <server>
      <username>${env.ARTIFACTORY_USER}</username>
      <password>${env.ARTIFACTORY_API_KEY}</password>
      <id>eri-releases</id>
    </server>
    <server>
      <username>${env.ARTIFACTORY_USER}</username>
      <password>${env.ARTIFACTORY_API_KEY}</password>
      <id>eri-snapshots</id>
    </server>
  </servers>
  <profiles>
    <profile>
      <repositories>
        <repository>
          <releases />
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>eri-releases</id>
          <url>https://elderresearch.jfrog.io/elderresearch/libs-release</url>
        </repository>
        <repository>
          <releases>
            <enabled>false</enabled>
          </releases>
          <snapshots />
          <id>eri-snapshots</id>
          <url>https://elderresearch.jfrog.io/elderresearch/libs-snapshot</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <releases />
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>eri-releases</id>
          <url>https://elderresearch.jfrog.io/elderresearch/libs-release</url>
        </pluginRepository>
        <pluginRepository>
          <releases>
            <enabled>false</enabled>
          </releases>
          <snapshots />
          <id>eri-snapshots</id>
          <url>https://elderresearch.jfrog.io/elderresearch/libs-snapshot</url>
        </pluginRepository>
      </pluginRepositories>
      <id>eri-profile</id>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>eri-profile</activeProfile>
  </activeProfiles>
</settings>
