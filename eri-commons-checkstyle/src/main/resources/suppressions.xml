	<suppress checks="MultipleStringLiterals" files=".*[\\/]src[\\/]test[\\/]"/>
	<suppress checks="TrailingComment" files=".*[\\/]src[\\/]test[\\/]"/>
	
	<suppress checks="." files=".*[\\/]target[\\/]generated-sources[\\/]"/>
</suppressions>			